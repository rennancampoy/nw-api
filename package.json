{
  "name": "nw-api",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 1 ./views/*.* dist/views/",
    "build": "yarn clean && tsc && yarn copy-files",
    "start": "npm run clean && tsc && npm run copy-files && node --es-module-specifier-resolution=node ./dist/app.js"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^9.0.2",
    "express": "~4.16.1",
    "express-react-views": "^0.11.0",
    "mongodb": "^3.6.7",
    "mongoose": "^5.12.11",
    "morgan": "~1.9.1",
    "pg": "^8.6.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.6.2",
    "sequelize-typescript": "^2.1.0",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.11",
    "@types/marko": "^4.6.2",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.6.0",
    "@types/react": "^17.0.6",
    "@types/validator": "^13.1.3",
    "copyfiles": "^2.4.1",
    "rimraf": "^3.0.2",
    "sequelize-cli": "^6.2.0"
  }
}
